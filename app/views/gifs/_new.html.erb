<h1 class="text-center">New Gif</h1>

<div id="gif-errors">

</div>
<%= form_for @gif, remote:true, html: {class: "form-horizontal"} do |f| %>
  <div class="form-group">
    <%= f.label :title, class: "col-xs-offset-2 col-xs-2 control-label" %>
    <div class="col-xs-5">
      <%= f.text_field :title, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :url, class: "col-xs-offset-2 col-xs-2 control-label" %>
    <div class="col-xs-5">
      <%= f.text_field :url, class: "form-control" %>
    </div>
  </div>
  <div class="row">
    <%= f.submit "Submit", class: "center-block" %>
  </div>
<% end %>
